var p1 = g.createPlace('p1',1);
var p2 = g.createPlace('p2',0);




var p3 = g.createPlace('p3',1);
var p4 = g.createPlace('p4',0);

var t1 = g.createTransition('t1');

var t2 = g.createTransition('t2');


p1.connect(t1,1);
t1.connect(p2,1);
t1.connect(p1,1);


p3.connect(t2,1);
t2.connect(p3,1);
t2.connect(p4,1);
